class Song(object):
    def __init__(self, lyrics):
        self.lyrics = lyrics


    def sing_me_a_song(self):
        for line in self.lyrics:
            print line

birthday_song = Song(["Happy B'day to you?"])
favourit_vine = Song(["BB ki Vine"])

birthday_song.sing_me_a_song()

favourit_vine.sing_me_a_song()
